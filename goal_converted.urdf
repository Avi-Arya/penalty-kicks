<?xml version="1.0"?>
<robot name="goal">
    <!-- Base link -->
    <link name="base">
        <inertial>
            <mass value="1.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
        </inertial>
    </link>

    <!-- Left Post -->
    <link name="left_post">
        <visual>
            <geometry>
                <box size="0.0625 0.0625 0.5"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
        <collision>
            <geometry>
                <box size="0.0625 0.0625 0.5"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>
    <joint name="left_post_joint" type="fixed">
        <parent link="base"/>
        <child link="left_post"/>
        <origin xyz="-0.03125 0.53125 0" rpy="0 0 0"/>
    </joint>

    <!-- Right Post -->
    <link name="right_post">
        <visual>
            <geometry>
                <box size="0.0625 0.0625 0.5"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
        <collision>
            <geometry>
                <box size="0.0625 0.0625 0.5"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>
    <joint name="right_post_joint" type="fixed">
        <parent link="base"/>
        <child link="right_post"/>
        <origin xyz="-0.03125 -0.53125 0" rpy="0 0 0"/>
    </joint>

    <!-- Crossbar -->
    <link name="crossbar">
        <visual>
            <geometry>
                <box size="0.0625 1.126 0.0625"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
        <collision>
            <geometry>
                <box size="0.0625 1.126 0.0625"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.2"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0.0" iyy="0.002" iyz="0.0" izz="0.002"/>
        </inertial>
    </link>
    <joint name="crossbar_joint" type="fixed">
        <parent link="base"/>
        <child link="crossbar"/>
        <origin xyz="-0.03125 0 0.28125" rpy="0 0 0"/>
    </joint>

    <!-- Center Net -->
    <link name="center_net">
        <visual>
            <geometry>
                <box size="0.005 1.0625 0.3625"/>
            </geometry>
            <material name="net_material">
                <color rgba="0.8 0.8 0.8 1"/>
            </material>
        </visual>
        <collision>
            <geometry>
                <box size="0.005 1.0625 0.3625"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.05"/>
            <inertia ixx="0.0005" ixy="0.0" ixz="0.0" iyy="0.0005" iyz="0.0" izz="0.0005"/>
        </inertial>
    </link>
    <joint name="center_net_joint" type="fixed">
        <parent link="base"/>
        <child link="center_net"/>
        <origin xyz="0.25 0 0" rpy="0 0 0"/>
    </joint>

    <!-- Additional Left Post Near Net -->
    <link name="left_post_near_net">
        <visual>
            <geometry>
                <box size="0.0625 0.0625 0.5"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
        <collision>
            <geometry>
                <box size="0.0625 0.0625 0.5"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>
    <joint name="left_post_near_net_joint" type="fixed">
        <parent link="center_net"/>
        <child link="left_post_near_net"/>
        <origin xyz="0 0.53125 0" rpy="0 0 0"/>
    </joint>

    <!-- Additional Right Post Near Net -->
    <link name="right_post_near_net">
        <visual>
            <geometry>
                <box size="0.0625 0.0625 0.5"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
        <collision>
            <geometry>
                <box size="0.0625 0.0625 0.5"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>
    <joint name="right_post_near_net_joint" type="fixed">
        <parent link="center_net"/>
        <child link="right_post_near_net"/>
        <origin xyz="0 -0.53125 0" rpy="0 0 0"/>
    </joint>

    <!-- Diagonal Connector Between Left Posts -->
    <link name="left_diagonal_connector">
        <visual>
            <geometry>
                <cylinder length="0.532" radius="0.03125"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.532" radius="0.03125"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>
    <joint name="left_diagonal_connector_joint" type="fixed">
        <parent link="left_post"/>
        <child link="left_diagonal_connector"/>
        <origin xyz="0 0 0.25" rpy="0 1.5708 0"/>
    </joint>

    <joint name="left_diagonal_connector_to_near_net" type="fixed">
        <parent link="left_diagonal_connector"/>
        <child link="left_post_near_net"/>
        <origin xyz="0 0 -0.25" rpy="0 1.5708 0"/>
    </joint>

    <!-- Diagonal Connector Between Right Posts -->
    <link name="right_diagonal_connector">
        <visual>
            <geometry>
                <cylinder length="0.532" radius="0.03125"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>
        <collision>
            <geometry>
                <cylinder length="0.532" radius="0.03125"/>
            </geometry>
        </collision>
        <inertial>
            <mass value="0.1"/>
            <inertia ixx="0.001" ixy="0.0" ixz="0.0" iyy="0.001" iyz="0.0" izz="0.001"/>
        </inertial>
    </link>
    <joint name="right_diagonal_connector_joint" type="fixed">
        <parent link="right_post"/>
        <child link="right_diagonal_connector"/>
        <origin xyz="0 0 0.25" rpy="0 1.5708 0"/>
    </joint>

    <joint name="right_diagonal_connector_to_near_net" type="fixed">
        <parent link="right_diagonal_connector"/>
        <child link="right_post_near_net"/>
        <origin xyz="0 0 -0.25" rpy="0 1.5708 0"/>
    </joint>
</robot>
